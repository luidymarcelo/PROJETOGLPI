    <connection>SQL_Glpi</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT 
    glpi_tickets.id AS ID_CHAMADOS,
    glpi_tickets.name AS TITULO, 
    DATE(glpi_tickets.date) AS OPEN_DATA,
    DATE(glpi_tickets.closedate) AS CLOSE_DATA,
    DATE(glpi_tickets.solvedate) AS SOLUCAO_DATA,
    DATE(glpi_tickets.date_mod) AS MOD_DATA,
    glpi_tickets.users_id_lastupdater AS ID_USER_UPDT,
    glpi_tickets.status AS ID_STATUS,
    glpi_tickets.users_id_recipient AS ID_USER_REQUE,
    glpi_tickets.requesttypes_id AS ID_ORIGEM,
    glpi_tickets.urgency AS ID_URGENCIA,
    glpi_tickets.impact AS ID_IMPACTO,
    glpi_tickets.priority AS ID_PRIORIDADE,
    glpi_tickets.itilcategories_id AS ID_CATEGORIAS,
    glpi_tickets.type AS ID_TIPO,
    DATE(glpi_tickets.begin_waiting_date) AS ATENDIMENTO_DATA,
    glpi_tickets.locations_id AS ID_LOCALIZACAO,
    CASE
    	WHEN glpi_itilsolutions.solutiontypes_id IS NULL
    		THEN '50'
    	ELSE glpi_itilsolutions.solutiontypes_id
    END AS ID_TIPO_SOLUTION,
    NULL AS SLA,
    DATEDIFF(DATE(solvedate),DATE(date)) AS TEMPO_SOLUCAO,
    CASE
    	WHEN glpi_tickets.solvedate IS NULL
    		THEN DATEDIFF(DATE(NOW()),DATE(date))
    ELSE NULL
    END AS TEMPO_SEM_SOLUCAO,
    CASE
    	WHEN glpi_itilsolutions.status IN ('2','3')
    		THEN glpi.glpi_itilsolutions.users_id
    ELSE NULL
    END AS ID_USER_SOLVE,
	/*
    CAST(SEC_TO_TIME(actiontime) AS CHAR) AS TIME_DURACAO_TOTAL,
	CAST(SEC_TO_TIME(solve_delay_stat) AS CHAR) AS TIME_SOLUCAO,
	CAST(SEC_TO_TIME(close_delay_stat) AS CHAR) AS TIME_FECHAMENTO,
	CAST(SEC_TO_TIME(takeintoaccount_delay_stat) AS CHAR) AS TIME_ATRIBUICAO,
	CAST(SEC_TO_TIME(waiting_duration) AS CHAR) AS TIME_PENDENTE
	*/
	ROUND((glpi_tickets.actiontime / 60 / 60),2) AS TIME_DURACAO_TOTAL,
	ROUND((glpi_tickets.solve_delay_stat / 60 / 60),2) AS TIME_SOLUCAO,
	ROUND((glpi_tickets.close_delay_stat / 60 / 60),2) AS TIME_FECHAMENTO,
	ROUND((glpi_tickets.takeintoaccount_delay_stat / 60 / 60),2) AS TIME_ATRIBUICAO,
	ROUND((glpi_tickets.waiting_duration / 60 / 60),2) AS TIME_PENDENTE
FROM glpi.glpi_tickets
LEFT JOIN glpi.glpi_itilsolutions
    ON glpi_tickets.id = glpi_itilsolutions.items_id
WHERE glpi_tickets.date >= '2024-01-01' 
	AND glpi_tickets.is_deleted &lt;&gt; '1'
    AND (glpi.glpi_itilsolutions.status IN ('2','3') AND glpi.glpi_itilsolutions.users_id >= '0' 
    OR glpi.glpi_itilsolutions.status IS NULL AND glpi.glpi_itilsolutions.users_id IS NULL
    OR glpi.glpi_itilsolutions.status = '4' AND glpi_tickets.solvedate IS NULL)
	AND glpi_tickets.users_id_recipient IS NOT NULL
	AND glpi_tickets.itilcategories_id > 0
GROUP BY glpi_tickets.id;</sql>
