    <connection>SQL_Glpi</connection>
    <execute_each_row>N</execute_each_row>
    <limit>0</limit>
    <sql>SELECT
    ID_PRIORIDADE,
    CASE ID_PRIORIDADE
        WHEN '1' THEN 'MUITO BAIXA'
        WHEN '2' THEN 'BAIXA'
        WHEN '3' THEN 'MÉDIA'
        WHEN '4' THEN 'ALTA'
        WHEN '5' THEN 'MUITO ALTA'
        WHEN '6' THEN 'CRITICA'
        ELSE 'Não Identicada'
    END AS PRIORIDADE
FROM (
    SELECT DISTINCT glpi_tickets.priority AS ID_PRIORIDADE
    FROM glpi.glpi_tickets
    LEFT JOIN glpi.glpi_itilsolutions
        ON glpi_tickets.id = glpi_itilsolutions.items_id
    WHERE glpi_tickets.date >= '2024-01-01'
        AND glpi_tickets.is_deleted &lt;&gt; '1'
        AND (
            (glpi.glpi_itilsolutions.status IN ('2','3') AND glpi.glpi_itilsolutions.users_id >= '0') 
            OR (glpi.glpi_itilsolutions.status IS NULL AND glpi.glpi_itilsolutions.users_id IS NULL)
            OR (glpi.glpi_itilsolutions.status = '4' AND glpi_tickets.solvedate IS NULL)
        )
        AND glpi_tickets.users_id_recipient > 0
        AND glpi_tickets.itilcategories_id > 0
) AS Prioridades</sql>
    <variables_active>N</variables_active>
